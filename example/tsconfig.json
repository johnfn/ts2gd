    "experimentalDecorators": true,
    "target": "es5",
    "module": "commonjs",
    "noLib": true,
    "noEmit": true,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "skipLibCheck": false,
    "baseUrl": ".",
